*{margin:0px;padding:0px;text-align:left;}
html{height:100%;}
img, svg{border:none;}
section, header, nav, article, aside, footer{display:block;}
body{-webkit-font-smoothing:subpixel-antialiased !important;-webkit-backface-visibility:hidden;-moz-backface-visibility:hidden;-ms-backface-visibility:hidden;}

input, textarea {
-webkit-appearance: none;
-webkit-border-radius: 0;
}

button:active, button:focus {
  outline: none;
}
button::-moz-focus-inner {
  border: 0;
}

input[type="text"]:focus { outline: none; }
input[type="number"]:focus { outline: none; }
input[type="submit"]:focus { outline: none; }
textarea:focus { outline: none; }

h1{font-family: 'Montserrat', sans-serif;font-weight:400;font-size:21px;text-align:center;color:#fff;padding:32px 0;}

.grid_1, .grid_2, .grid_3, .grid_3_1, .grid_4, .grid_5, .grid_6, .grid_6_1, .grid_7, .grid_8, .grid_9, .grid_10, .grid_11, .grid_12, .grid_13, .grid_14, .grid_15, .grid_16, .grid_17, .grid_18, .grid_19, .grid_20, .grid_21, .grid_22, .grid_23, .grid_24, .grid_6_m0 {display:inline;float:left;position:relative;}

.top_40{margin-top:40px;}
.top_20{margin-top:20px;}
.bottom_40{margin-bottom:40px;}
.overflow_hidden{overflow:hidden;}

.bg{background: linear-gradient(180deg, #182245 0%, #171B3C 19.79%, #161130 41.15%, #15021C 100%, #15021C 100%, #15021C 100%);margin:0 auto;min-width:400px;}

.stop-scrolling {height: 100%;overflow: hidden;}

